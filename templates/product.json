            "liquid": "<style>\n  .downloads-section {\n    display: flex;\n    justify-content: center;\n    align-items: center;\n    text-align: center;\n    background-color: #F6F6F6;\n    padding-top: 20px; \/* oberer Abstand *\/\n    padding-bottom: 20px; \/* unterer Abstand *\/\n  }\n<\/style>\n\n<div class=\"downloads-section\">\n  <div class=\"content\">\n    {% assign pdp_downloads = product.metafields.custom.pdp_downloads.value %}\n    \n    {% if pdp_downloads %}\n      <h2>{{ pdp_downloads.linktitel }}<\/h2>\n      \n      {% if pdp_downloads.datei %}\n        <a href=\"{{ pdp_downloads.datei }}\" download>\n          Download\n        <\/a>\n      {% endif %}\n    {% endif %}\n  <\/div>\n<\/div>",
